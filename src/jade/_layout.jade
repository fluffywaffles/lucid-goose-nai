doctype html
html
  head
    meta(charset='utf-8')
    title NAI App
    meta(name='description', content='NAI Technician Assignment App')
    meta(name='viewport', content='width=device-width, initial-scale=1')

    link(rel='stylesheet', href='styles/main.css')

  body
    !='<!--[if lte IE 9]>'
    p.browsehappy
      | You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.
    !='\n<![endif]-->'

    block app

    script(src='https://cdnjs.cloudflare.com/ajax/libs/riot/2.3.11/riot.js')

    // build:js scripts/vendor.js
    script(src='bower_components/xml2json/xml2json.js')
    script(src='bower_components/axios/dist/axios.js')
    // endbuild

    // app setup
    script(src='scripts/app.bundle.js')

    // build:js scripts/tags.js
    script(src='tags/account-nav.tag.js')
    script(src='tags/main.tag.js')
    script(src='tags/projects.tag.js')
    script(src='tags/project-details.tag.js')
    script(src='tags/technician-list.tag.js')
    script(src='tags/technician.tag.js')
    // endbuild

    // app main
    script(src='scripts/main.bundle.js')

    include includes/google-analytics.jade
